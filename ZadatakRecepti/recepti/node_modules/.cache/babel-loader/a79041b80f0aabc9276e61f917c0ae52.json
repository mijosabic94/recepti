{"ast":null,"code":"import ViewRecept from \"./ViewRecept.vue\";\nimport EditRecept from \"./EditRecept.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"HomeItem\",\n\n  data() {\n    return {\n      rows: [],\n      selectedRecept: {},\n      showRecept: false,\n      showEditRecept: false,\n      searchText: ''\n    };\n  },\n\n  components: {\n    ViewRecept,\n    EditRecept\n  },\n\n  created() {\n    axios.get(\"http://localhost:3000/api/getAllRecords\").then(response => {\n      this.rows = response.data;\n      this.rowsShow = response.data;\n    }).catch(e => {\n      this.errors.push(e);\n    });\n  },\n\n  computed: {\n    rowsShow() {\n      if (this.searchText === '') {\n        return this.rows;\n      }\n\n      return this.rows.filter(e => e.Naziv.includes(this.searchText));\n    }\n\n  },\n  methods: {\n    openRecept: function (row) {\n      this.showRecept = false;\n      this.selectedRecept = row;\n      this.showRecept = true;\n    },\n    handleEditButton: function () {\n      this.showEditRecept = true;\n      this.showRecept = false;\n    },\n    handleRemoveButton: function () {\n      var item = {};\n      item[\"_id\"] = 'ObjectId(\"' + this.selectedRecept._id + '\")';\n\n      var axios = require(\"axios\");\n\n      var config = {\n        method: \"post\",\n        url: \"http://localhost:3000/api/removeRecord\",\n        headers: {\n          search_item: JSON.stringify(item),\n          \"Access-Control-Allow-Origin\": \"http://localhost:8080\"\n        }\n      };\n      axios(config).then(function (response) {\n        console.log(JSON.stringify(response.data));\n      }).catch(function (error) {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,UAAP,MAAuB,kBAAvB;AACA,OAAOC,UAAP,MAAuB,kBAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,oBAAc,EAAE,EAFX;AAGLC,gBAAU,EAAE,KAHP;AAILC,oBAAc,EAAE,KAJX;AAKLC,